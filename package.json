{
  "name": "fpjc",
  "version": "1.0.0",
  "description": "Functional utilities library for js/coffee",
  "keywords": [
    "functional programming",
    "coffeescript",
    "javascript",
    "library",
    "fpc",
    "fp"
  ],
  "files": [
    "lib/coffee/fpjc.coffee",
    "lib/fpjc.js",
    "lib/fpjc.cjs",
    "lib/min/fpjc.min.js",
    "lib/min/fpjc.min.cjs"
  ],
  "author": {
    "name": "fxhxyz4",
    "email": "fxhxyz@proton.me",
    "url": "https://fxhxyz.vercel.app/"
  },
  "homepage": "https://github.com/fxhxyz4/fpjc",
  "bugs": "https://github.com/fxhxyz4/fpjc/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/fxhxyz4/fpjc.git"
  },
  "maintainers": [
    {
      "name": "fxhxyz4",
      "email": "fxhxyz@proton.me"
    }
  ],
  "type": "module",
  "license": "MIT",
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "scripts": {
    "lint-config": "coffeelint --makeconfig > coffeelint.json",
    "lint": "coffeelint -f coffeelint.json lib/coffee/*.coffee examples/*.coffee",
    "prettier": "prettier lib/coffee/*.coffee examples/*.coffee tests/*.coffee --write",
    "babel-dev": "babel lib/*.* -d lib/ --presets=@babel/preset-env",
    "babel-prod": "babel lib/min/*.js && lib/min/*.cjs -d lib/min/ --presets=@babel/preset-env",
    "uglify-js": "uglifyjs lib/min/*.js -o lib/min/*.js --ie --toplevel",
    "uglify-cjs": "uglifyjs lib/min/*.cjs -o lib/min/*.cjs --ie --toplevel",
    "uglify": "npm run uglify-js && npm run uglify-cjs",
    "babel": "npm run babel-dev && npm run babel-prod",
    "build-dev": "NODE_ENV=dev rollup -c",
    "build-prod": "NODE_ENV=prod rollup -c",
    "build": "npm run build-dev && npm run build-prod",
    "test-c": "coffee examples/add.coffee",
    "test-j": "node examples/lol.mjs",
    "test": "echo \"tested\""
  },
  "devDependencies": {
    "@babel/cli": "7.23.0",
    "@babel/core": "7.23.2",
    "@babel/preset-env": "7.23.2",
    "@coffeelint/cli": "^5.2.11",
    "babel-minify": "0.5.2",
    "coffeescript": "2.7.0",
    "prettier-plugin-coffeescript": "^0.1.5",
    "rollup": "4.1.4",
    "rollup-plugin-coffee-script": "2.0.0",
    "rollup-plugin-uglify": "4.0.0",
    "uglify-js": "3.17.4"
  },
  "engines": {
    "node": "20.x"
  }
}

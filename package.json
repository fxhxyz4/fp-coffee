{
  "name": "fp-coffee",
  "version": "1.0.0",
  "description": "Functional utilities library for coffee/js",
  "keywords": [
    "functional programming",
    "coffeescript",
    "javascript",
    "library",
    "fp"
  ],
  "author": {
    "name": "fxhxyz4",
    "email": "fxhxyz@proton.me",
    "url": "https://fxhxyz.vercel.app/"
  },
  "homepage": "https://github.com/fxhxyz4/fp-coffee",
  "bugs": "https://github.com/fxhxyz4/fp-coffee/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/fxhxyz4/fp-coffee.git"
  },
  "maintainers": [
    {
      "name": "fxhxyz4",
      "email": "fxhxyz@proton.me"
    }
  ],
  "license": "MIT",
  "scripts": {
    "lint-config": "coffeelint --makeconfig > coffeelint.json",
    "lint": "coffeelint -f coffeelint.json ./src/*.coffee ./examples/*.coffee ./dist/*.coffee",
    "prettier": "prettier ./src/*.coffee ./examples/*.coffee ./dist/*.coffee --write",
    "babel": "coffee --no-header --compile --transpile --inline-map ./dist/fpcoffee.coffee",
    "webpack": "webpack -c ./webpack.config.js --mode=production",
    "webpack-watch": "webpack -c ./webpack.config.js --mode=development -w",
    "test": "coffee ./examples/add.coffee"
  },
  "devDependencies": {
    "@babel/cli": "7.23.0",
    "@babel/core": "7.23.2",
    "@babel/preset-env": "7.23.2",
    "@coffeelint/cli": "^5.2.11",
    "coffee-loader": "4.0.0",
    "coffeescript": "2.7.0",
    "glob": "10.3.10",
    "prettier-plugin-coffeescript": "^0.1.5",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-concat-files-plugin": "0.5.2"
  }
}

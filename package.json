{
  "name": "fpc",
  "version": "1.0.0",
  "description": "Functional utilities library for coffee/js",
  "keywords": [
    "functional programming",
    "coffeescript",
    "javascript",
    "library",
    "fpc",
    "fp"
  ],
  "author": {
    "name": "fxhxyz4",
    "email": "fxhxyz@proton.me",
    "url": "https://fxhxyz.vercel.app/"
  },
  "homepage": "https://github.com/fxhxyz4/fpc",
  "bugs": "https://github.com/fxhxyz4/fpc/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/fxhxyz4/fpc.git"
  },
  "maintainers": [
    {
      "name": "fxhxyz4",
      "email": "fxhxyz@proton.me"
    }
  ],
  "type": "module",
  "license": "MIT",
  "scripts": {
    "lint-config": "coffeelint --makeconfig > coffeelint.json",
    "lint": "coffeelint -f coffeelint.json ./src/*.coffee ./examples/*.coffee ./dist/*.coffee",
    "prettier": "prettier ./src/*.coffee ./examples/*.coffee ./dist/*.coffee --write",
    "babel": "babel dist/*.* -d dist/ --presets=@babel/preset-env",
    "build-dev": "NODE_ENV='dev' rollup -c",
    "build-prod": "NODE_ENV='prod' rollup -c",
    "test-c": "coffee ./examples/add.coffee",
    "test-j": "node ./examples/lol.js"
  },
  "devDependencies": {
    "@babel/cli": "7.23.0",
    "@babel/core": "7.23.2",
    "@babel/plugin-transform-runtime": "7.23.2",
    "@babel/preset-env": "7.23.2",
    "@coffeelint/cli": "^5.2.11",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-node-resolve": "15.2.3",
    "coffeescript": "2.7.0",
    "prettier-plugin-coffeescript": "^0.1.5",
    "rollup": "4.1.1",
    "rollup-plugin-coffee-script": "2.0.0",
    "rollup-plugin-uglify": "6.0.4"
  },
  "engines": {
    "node": "16.x"
  }
}
